package edu.uncc.cci.mobileapps;

public class Bill {
	String name;
	int id;
	double price;
	int quantity;
	@Override
	public String toString() {
		return "Bill [name=" + name + ", id=" + id + ", price=" + price + ", quantity=" + quantity + "]";
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public double getPrice() {
		return price;
	}
	public void setPrice(double price) {
		this.price = price;
	}
	public int getQuantity() {
		return quantity;
	}
	public void setQuantity(int quantity) {
		this.quantity = quantity;
	}
	public Bill(String name, int id, double price, int quantity) {
		super();
		this.name = name;
		this.id = id;
		this.price = price;
		this.quantity = quantity;
	}
}
